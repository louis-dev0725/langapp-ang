{
    "extends": "../tslint.json",
    "rules": {
      "directive-selector": [
        true,
        "attribute",
        "app",
        "camelCase"
      ],
      "component-selector": [
        true,
        "element",
        "app",
        "kebab-case"
      ],
      "no-redundant-jsdoc": false,
      "radix": false,
      "comment-format": [
        true,
        "check-lowercase",
        "check-uppercase"
      ]
    }
}
