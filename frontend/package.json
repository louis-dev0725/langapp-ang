{
  "name": "langapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "cross-env NODE_ENV=production ng build",
    "build:stats": "cross-env NODE_ENV=production ng build --stats-json --sourceMap=true",
    "bundle-report": "cross-env NODE_ENV=production webpack-bundle-analyzer ./dist/langapp/stats.json",
    "ionic:build": "cross-env NODE_ENV=production ng build --configuration=ionic,production",
    "ionic:serve": "ng serve --serve-path / --configuration=ionic,development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "webpack-bundle-analyzer dist/langapp/stats-es5.json",
    "translate-extract": "./node_modules/.bin/ngx-translate-extract -i ./src -o ./src/assets/i18n/*.json --format namespaced-json --marker _ --clean --sort",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "private": true,
