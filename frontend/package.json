{
  "name": "langapp",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot=true --build-optimizer=true --output-hashing=all",
    "build:stats": "ng build --prod --aot=true --build-optimizer=true --stats-json --sourceMap=true",
    "bundle-report": "webpack-bundle-analyzer ./dist/langapp/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "webpack-bundle-analyzer dist/langapp/stats-es5.json",
    "translate-extract": "./node_modules/.bin/ngx-translate-extract -i ./src -o ./src/assets/i18n/*.json --format namespaced-json --marker _ --clean --sort"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/material-moment-adapter": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@fontsource/open-sans": "^4.2.1",
    "@fullcalendar/core": "^5.5.1",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngrx/data": "^11.0.0",
    "@ngrx/effects": "^11.0.0",
    "@ngrx/entity": "^11.0.0",
    "@ngrx/store": "^11.0.0",
    "@ngrx/store-devtools": "^11.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@types/video.js": "^7.3.14",
    "@videojs/themes": "^1.0.1",
    "chart.js": "^2.9.4",
    "core-js": "^3.8.3",
    "jstz": "^2.1.1",
    "ngx-captcha": "^9.0.1",
    "prettier": "^2.2.1",
    "primeflex": "2.0.0",
    "primeicons": "4.1.0",
    "primeng": "^11.2.1",
    "quill": "^1.3.7",
    "reflect-metadata": "~0.1.12",
    "rxjs": "^6.6.3",
    "tns-core-modules": "~6.5.25",
    "tslib": "^2.1.0",
    "video.js": "^7.11.4",
