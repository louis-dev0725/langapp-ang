{
  "name": "call-rocket",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-stats": "ng build --prod --aot=true --buildOptimizer=true --stats-json",
    "bundle-report": "webpack-bundle-analyzer ./dist/call-rocket/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
